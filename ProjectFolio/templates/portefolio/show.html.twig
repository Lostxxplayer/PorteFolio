{% extends 'base.html.twig' %}
{% block body %}
<projects>
  <h2>{{ project.title }}</h2>
    <div class="metadata">Publier le {{ project.createdAt | date('d/m/Y') }} a {{ project.createdAt | date ('H:i') }} dans la categorie {{ project.category.title }}</div>
    <div class="content">
        <img src="{{ project.image }}" alt="">
        {{ project.content | raw }}
        <a href="{{ path('Project_edit',{'id': project.id}) }}" class="btn btn-primary">Editer le project</a>
       
    
    

        
    </div>
</projects>
{% endblock %}